<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#a05890ea" />
            <gradient android:angle="-90" android:startColor="#90144dfc" android:endColor="#a05890ea"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#5e89ed" />
            <solid android:color="#7890e9"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#a05890ea" />
            <gradient android:angle="-90" android:startColor="#90144dfc" android:endColor="#a05890ea" />
        </shape>
    </item>
</selector>